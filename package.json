{
  "name": "holo-apollo-ui",
  "version": "1.0.0",
  "description": "Holo Apollo UI",
  "engines": {
    "node": "11.9.0",
    "yarn": "1.13.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production babel-node server -r scripts/serverPreconfig.js",
    "start:dev": "nodemon --watch server --watch webpack.dev.config.js --watch .env --exec 'cross-env NODE_ENV=development babel-node server -r scripts/serverPreconfig.js'",
    "prebuild": "yarn build:clean",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:dev": "cross-env NODE_ENV=development webpack --config webpack.dev.config.js --color -p --progress --hide-modules",
    "build:clean": "rimraf ./static/bundles",
    "test": "jest --config=jest.config.js",
    "test:coverage": "yarn test --coverage",
    "eslint": "eslint src -f tap",
    "flow": "flow",
    "storybook": "start-storybook -p 9001 -c storybook/config",
    "storybook:build": "build-storybook -c storybook/config -o storybook/static",
    "build-langs": "babel-node scripts/mergeMessages.js"
  },
  "keywords": [],
  "author": "Valerii Kovalchuk",
  "repository": {
    "type": "git",
    "url": "https://github.com/holo-apollo/holo-apollo-ui"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
